    "sourcemap-codec": "^1.4.1",
    "svelte-extras": "^2.0.2",
    "svelte-page": "^1.0.1",
    "svelte-routing": "^0.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
