    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-prismjs": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
