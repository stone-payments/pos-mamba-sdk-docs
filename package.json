{
  "name": "@mamba/docs",
  "description": "Docs and examples for Mamba Web",
  "version": "1.1.0",
  "author": "Stone Payments - Mamba Team",
  "license": "Apache-2.0",
  "homepage": "https://mambasdk-docs.stone.com.br",
  "repository": {
    "type": "git",
    "url": "https://github.com/stone-payments/pos-mamba-sdk-docs"
  },
  "scripts": {
    "dev": "sapper dev NODE_ENV=development",
    "dev:debug": "rm -rf __sapper__/ && node --inspect-brk node_modules/sapper/dist/cli.js dev NODE_ENV=development",
    "build:dev": "cross-env NODE_ENV=development sapper build",
    "build": "cross-env NODE_ENV=production sapper build",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "now-start": "node scripts/slack_webhook.js && node build",
    "deploy:staging": "now --public && now alias",
    "deploy": "deploy/initiate.sh",
    "pull:packages": "node scripts/update_packages.js ",
    "pull:packages:bash": "ls ./ && chmod +x scripts/update_packages.sh && scripts/update_packages.sh && ls ./packages",
    "postinstall": "npm run pull:packages && ls ./packages/components",
    "lint": "npm run lint:js",
    "setup": "rm -rf node_modules | npm install",
    "lint:js": "eslint \"*.js\" \"{tools,packages,docs-sapper}/**/*.{js,html,svelte}\"",
    "format": "npm run format:css; npm run format:js",
    "format:js": "prettier --loglevel silent --write \"*.js\" \"{tools,packages,docs-sapper}/**/*.js\" && eslint --fix \"*.js\" \"{tools,packages,docs-sapper}/**/*.{js,html,svelte}\"",
    "format:css": "prettier --loglevel silent --write \"packages/**/*.{css,pcss}\"",
    "clean:deployments": "chmod +x scripts/now_clean.sh && scripts/now_clean.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,svelte}": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{pcss,css}": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@mamba/appbar": "latest",
    "@mamba/barcode": "latest",
    "@mamba/button": "latest",
    "@mamba/collection": "latest",
    "@mamba/dialog": "latest",
    "@mamba/icon": "latest",
    "@mamba/input": "latest",
    "@mamba/pos": "latest",
    "@mamba/printable": "latest",
    "@mamba/progress": "latest",
    "@mamba/qrcode": "latest",
    "@mamba/range": "latest",
    "@mamba/sprite": "latest",
    "@mamba/switch": "latest",
    "@mamba/tabs": "latest",
    "cheerio": "^1.0.0-rc.2",
    "codemirror": "^5.40.2",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "degit": "^2.1.3",
    "do-not-zip": "^0.1.1",
    "dotenv": "^6.0.0",
    "encoding": "^0.1.12",
    "express": "^4.16.3",
    "globby": "^8.0.1",
    "golden-fleece": "^1.0.9",
    "jsbarcode": "^3.11.0",
    "locate-character": "^2.0.5",
    "lodash-fp": "^0.10.4",
    "lodash.capitalize": "^4.2.1",
    "marked": "^0.5.0",
